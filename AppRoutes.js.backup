                        {/* <AuthRoute path="/attendance" type="private" exact>
                          <AttendanceRecap />
                        </AuthRoute> */}
                        <AuthRoute path="/attendance"
                          component={AttendanceHistoryAll}
                          type="private" exact
                        />
                        {/* <AuthRoute path="/attendance" type="private" exact>
                      <AttendanceUser />
                    </AuthRoute> */}
                        {/* <AuthRoute path="/attendance/history" type="private" >
                      <AttendanceHistory />
                    </AuthRoute> */}
                        {/* <AuthRoute path="/attendance/recap/:id" type="private" exact render={(props) => <AttendanceRecapDetail {...props} />} />
                        <AuthRoute path="/attendance/recap" type="private">
                          <AttendanceRecap />
                        </AuthRoute> */}
                        <AuthRoute path="/calendar" type="private" exact>
                          <CalendarMenu />
                        </AuthRoute>
                        <AuthRoute path="/company" type="private" exact>
                          <CompanyMenu />
                        </AuthRoute>
                        <AuthRoute path="/company/pengaturan-dasar" type="private" exact>
                          <PengaturanDasar />
                        </AuthRoute>
                        <AuthRoute path="/company/pengaturan-jabatan" type="private" exact>
                          <PengaturanJabatan />
                        </AuthRoute>
                        <AuthRoute path="/company/pengaturan-profil-perusahaan" type="private" exact>
